{
	"name": "taroDemo",
	"version": "0.0.1",
	"code": 0,
	"private": true,
	"scripts": {
		"dev": "react-native run-android",
		"ios": "cd ios && xcodebuild archive -project gasasst.xcodeproj -configuration \"Release\" archive",
		"start": "react-native start",
		"build:app": "cd ./android/ && ./gradlew assembleRelease && cd ../",
		"buildjs:release": "cd ../app/ && npm version $npm_package_version && git checkout release && ./node_modules/.bin/taro build --type rn --platform android && git checkout main && cd ../gas-app-native-shell/",
		"buildjs:dev": "cd ../app/ && git checkout main && ./node_modules/.bin/taro build --type rn --platform android && cd ../app-native-shell/",
		"build:release": "yarn buildjs:release && yarn build:app && cp -r ./android/app/build/outputs/apk/release/app-arm64-v8a-release.apk /01factory/production$npm_package_version.apk",
		"build:dev": "yarn buildjs:dev && yarn build:app && cp -r ./android/app/build/outputs/apk/release/app-arm64-v8a-release.apk /01factory/develope$npm_package_version.apk",
		"build": "code=$(($npm_package_code+1)) && sed -i \"s/code\\\": $npm_package_code/code\\\": $code/\" ./package.json && yarn setVersion $npm_package_version $npm_package_code $code && yarn build:release && yarn build:dev && git commit -am 'package'",
		"test": "jest",
		"lint": "eslint .",
		"upgradePeerdeps": "install-peerdeps @tarojs/taro-rn -o -Y && install-peerdeps @tarojs/components-rn -o -Y && install-peerdeps @tarojs/router-rn -o -Y"
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "~1.15.11",
		"@react-native-community/cameraroll": "~4.1.2",
		"@react-native-community/clipboard": "~1.5.1",
		"@react-native-community/geolocation": "~2.0.2",
		"@react-native-community/masked-view": "~0.1.11",
		"@react-native-community/netinfo": "~7.1.2",
		"@react-native-community/slider": "~4.1.12",
		"@react-native-picker/picker": "~2.2.1",
		"expo-av": "~9.2.3",
		"expo-barcode-scanner": "~10.2.2",
		"expo-brightness": "~9.2.2",
		"expo-camera": "~11.2.2",
		"expo-file-system": "~11.1.3",
		"expo-image-picker": "~10.2.3",
		"expo-keep-awake": "~9.2.0",
		"expo-location": "^12.1.2",
		"expo-permissions": "~12.1.1",
		"expo-sensors": "~10.2.2",
		"react": "^17.0.2",
		"react-native": "^0.66.0",
		"react-native-device-info": "~8.4.8",
		"react-native-gesture-handler": "~1.10.3",
		"react-native-image-resizer": "~1.4.5",
		"react-native-pager-view": "~5.4.9",
		"react-native-reanimated": "~1.13.3",
		"react-native-safe-area-context": "~3.3.2",
		"react-native-screens": "~2.18.1",
		"react-native-svg": "~12.1.1",
		"react-native-syan-image-picker": "0.4.10",
		"react-native-unimodules": "~0.14.10",
		"react-native-webview": "~11.14.3"
	},
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/runtime": "^7.11.2",
		"@react-native-community/eslint-config": "^2.0.0",
		"@types/react": "^16.9.53",
		"babel-jest": "^26.6.3",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.5",
		"eslint": "^7.11.0",
		"install-peerdeps": "^3.0.3",
		"jest": "^26.6.3",
		"jest-junit": "^12.0.0",
		"jest-react-native": "^18.0.0",
		"metro-react-native-babel-preset": "^0.66.2",
		"react-dom": "^16.14.0",
		"react-native-bootsplash": "^3.2.6",
		"react-native-versioner": "^1.0.1",
		"react-test-renderer": "^17.0.1",
		"sinon": "^9.2.0"
	},
	"jest": {
		"preset": "react-native"
	}
}
